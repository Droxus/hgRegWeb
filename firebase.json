{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "site": "karta-pobytu-rejestracja",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/cancelorder",
        "destination": "/cancelorder.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
